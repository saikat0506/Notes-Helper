chmod +x sync.sh
./sync.sh


git syncnotes

git config --global alias.syncnotesmain&saikat '!f() { \
  git add . && \
  git commit -m "." && \
  git push origin saikat && \
  git checkout main && \
  git fetch upstream && \
  git merge upstream/main && \
  git push origin main --force-with-lease && \
  git checkout saikat && \
  git merge main && \
  git push origin saikat; }; f'


git config --global alias.syncnotessaikat '!f() { \
  git add . && \
  git commit -m "." && \
  git push origin saikat && \
  git checkout main && \
  git fetch upstream && \
  git merge upstream/main && \
  git push origin main && \
  git checkout saikat; }; f'


